{
    "name": "webee-online/g-cms",
    "description": "Simple CMS with a Git repository as a main content datastore",
    "version": "0.0.1",
    "type": "project",
    "license": "GPL-3.0-only",
    "homepage": "https://github.com/adam-webee/gCMS",
    "authors": [
        {
            "name": "Adam Wojciechowski",
            "email": "adam@webee.online",
            "homepage": "https://webee.online"
        }
    ],

    "require": {
        "symfony/config": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/console": "^5.1",
        "symfony/filesystem": "^5.1",
        "league/commonmark": "^1.5",
        "twig/twig": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "kahlan/kahlan": "^4.7",
        "symfony/var-dumper": "^5.1"
    },
    "autoload":{
        "psr-4": {
            "WeBee\\gCMS\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4": {
            "WeBee\\gCMSTests\\": "spec/"
        }
    },
    "bin": [
        "gcms"
    ]
}
